{
  "compilerOptions": {
    "noImplicitAny": true,
    "allowJs": true,
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "target": "ESNext",
    "removeComments": false,
    "declaration": false,
    "declarationMap": false,
    "inlineSourceMap": false,
    "inlineSources": false,
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "preserveConstEnums": false,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmit": true,
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "./"
  ]
}